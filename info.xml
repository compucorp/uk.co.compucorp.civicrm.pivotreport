<?xml version="1.0"?>
<extension key="uk.co.compucorp.civicrm.pivotreport" type="module">
  <file>pivotreport</file>
  <name>Pivot Report</name>
  <description>Report page with Pivot Table containing data of various CiviCRM entities.</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Compucorp</author>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/compucorp/uk.co.compucorp.civicrm.pivotreport</url>
    <url desc="Documentation">https://github.com/compucorp/uk.co.compucorp.civicrm.pivotreport</url>
    <url desc="Support">https://github.com/compucorp/uk.co.compucorp.civicrm.pivotreport/issues</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2025-09-09</releaseDate>
  <version>2.1.1</version>
  <develStage>Stable</develStage>
  <compatibility>
    <ver>5.65</ver>
  </compatibility>
  <comments/>
  <civix>
    <namespace>CRM/PivotReport</namespace>
    <format>23.02.1</format>
  </civix>
  <mixins>
    <mixin>menu-xml@1.0.0</mixin>
    <mixin>smarty-v2@1.0.1</mixin>
    <mixin>entity-types-php@1.0.0</mixin>
  </mixins>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <upgrader>CRM_PivotReport_Upgrader</upgrader>
</extension>
